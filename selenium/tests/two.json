{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.hnsearch.com/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "body-form-searchbox"
      },
