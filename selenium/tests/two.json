    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='body-wrapper']/form/input[2]"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "relevance"
    }
  ]
}